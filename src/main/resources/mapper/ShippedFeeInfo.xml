      </if>           
      <if test="items.unloadUnitPrice != null" >
        unload_unit_price,
      </if>           
      <if test="items.unloadUnitPrice != null" >
        unload_unit_price,
      </if>           
      <if test="items.unloadUnitPrice != null" >
        unload_unit_price,
      </if>           
      <if test="items.unloadUnitPrice != null" >
        unload_unit_price,
      </if>           
      
      
      
      
      
      <if test="items.createTime != null" >
        create_time,
      </if>
      <if test="items.updateTime != null" >
        update_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="item.clp != null" >
        #{item.clp,jdbcType=VARCHAR},
      </if>
      <if test="item.shippedDate != null" >
