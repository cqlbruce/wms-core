      </if>           
      <if test="item.createTime != null" >
        #{item.createTime,jdbcType=VARCHAR},
      </if>
      <if test="item.updateTime != null" >
        #{item.updateTime,jdbcType=VARCHAR},
      </if>            
    </trim>
